#include "main.h"

/**
 * *_strcat - appends the strig in *src into *dest
 *@dest: destination variable
 *@src: source variable
 *Return: destination variable
 */

char *_strcat(char *dest, char *src)
{
	len_dest;
	len_src;
	int i = 0;

	while (dest[i++])
		len_dest++;
	for (src[len_src] != '\0')
	{
		dest[len_dest++] = src[len_src++];
	}
	dest[i] = '0';
	Return(dest);
}
