#include "main.h"

/**
 *_memcpy - copy from source file to destination file
 *@dest: destination file
 *@src: source file
 *@n: variable
 *Return: destination
 */

char *_memcpy(char *dest, char *src, unsigned int n)
{
	unsigned int i;

	for (i = 0; i < n; i++)
		dest(i) = src(i);
	return (dest);
}
