#include <stdio.h>

/**
 * main -  program that prints all possible combinations of single-digit
 * Numbers must be separated by ,followed by a space
 * Numbers should be printed in ascending order
 * You can only use the putchar function
 * Return: 0
 */

int main(void)
{
	int f = 48;
	int hom = 44;
	while (f <= 57)
	{
		putchar(f);
		if (f != 57)
		{
			putchar(hom);
			putchar(32);
		}
		f += 1;
	}
	putchar('\n');
	return (0);
}
